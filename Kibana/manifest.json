{
    "name": "K",
    "version": "2.0.0",
    "manifest_version": 2,
    "description": "Kibana",
    "browser_action": {
        "default_title": "Kibana"
    },
    "background": {
        "persistent": true,
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "*://*/*",
        "tabs",
        "activeTab",
        "background",
        "downloads",
        "cookies",
        "storage",
        "unlimitedStorage",
        "notifications",
        "webRequest",
        "webRequestBlocking"
    ]
}